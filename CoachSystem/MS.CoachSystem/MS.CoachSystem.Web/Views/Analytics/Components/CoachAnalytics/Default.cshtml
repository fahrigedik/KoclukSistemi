@model MS.CoachSystem.Web.Protos.AnalyticsResponse

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Coach Analytics</h5>
        <div class="row">
            <div class="col-md-4">
                <div class="analytics-item">
                    <span class="analytics-value">@Model.TotalStudents</span>
                    <span class="analytics-label">Total Students</span>
                </div>
            </div>
            <div class="col-md-4">
                <div class="analytics-item">
                    <span class="analytics-value">@Model.TotalTargets</span>
                    <span class="analytics-label">Total Targets</span>
                </div>
            </div>
            <div class="col-md-4">
                <div class="analytics-item">
                    <span class="analytics-value">@Model.TotalMeetings</span>
                    <span class="analytics-label">Total Meetings</span>
                </div>
            </div>
        </div>
    </div>
</div>